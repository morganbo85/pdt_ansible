[Unit]
Description=Run ansible-pull to auto-provision
After=network-online.target
Wants=network-online.target

[Service]
Type=oneshot
User=root
ExecStart=/usr/bin/ansible-pull --accept-host-key -o -U git@github.com:morganbo85/ansible_dt.git playbooks/local.yml


[Install]
WantedBy=multi-user.target
